{
  "name": "xcode75/jobby",
  "homepage": "https://github.com/xcode75/jobby",
  "license": "MIT",
  "description": "Manage all your cron jobs without modifying crontab. Handles locking, logging, error emails, and more.",
  "authors": [
    {
      "name": "xcode75",
      "homepage": "https://github.com/xcode75"
    }
  ],
  "require": {
    "php": ">=7.4",
    "dragonmantank/cron-expression": "^3.0",
    "opis/closure": "^3.5",
    "symfony/process": "^2.7|^3.0|^4.0|^5.0"
  },
  "require-dev": {
    "mp091689/dump-die": "^1.0",
    "phpunit/phpunit": "^4.6",
    "symfony/filesystem": "^2.7|^3.0|^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "Jobby\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  }
}
